{
  "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "linebreak-style": ["error", "unix"],
      "no-console": "warn",
      "no-multiple-empty-lines": "error",
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "indent": [
          "error",
          2
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ]
  }
}
